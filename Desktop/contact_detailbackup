          <script>
            var iso_by_ip = document.getElementById('recv_iso').value;

            var isd_suggestor = new Suggester({"type":"isd","element":"country-dropdownmy","onSelect":selectCountryedit, fields: "cname,iso,icon_order", displayFields:"cname,value",displaySeparator:"  +","defaultValue":iso_by_ip});

            function selectCountryedit(event , ob)
            {
              $("#country-dropdown1 dt a span").attr('style','background-position:0px -'+ob.data.icon_order*11+'px');
              $("#country-dropdown1 dt span.value").html('+'+ob.value);
            }
            function selectCountryedit_sec(event , ob)
            {
              $("#country-dropdown_sec dt a span").attr('style','background-position:0px -'+ob.data.icon_order*11+'px');
              $("#country-dropdown_sec dt span.value").html('+'+ob.value);
            }
            $('.tabbb span').on('click', function() {
              $(this).addClass('active').siblings().removeClass('active');
            });
            $('.ad_edit_tabs nav a').on('click', function() {
              show_content($(this).index());
            });
            show_content(0);
            function show_content(index) {
              $('.ad_edit_tabs .content.visible').removeClass('visible');
              $('.ad_edit_tabs .content:nth-of-type(' + (index + 1) + ')').addClass('visible');
              $('.butnss').addClass('visible');
              $('.ad_edit_tabs nav a.selected').removeClass('selected');
              $('.ad_edit_tabs nav a:nth-of-type(' + (index + 1) + ')').addClass('selected');
            }
